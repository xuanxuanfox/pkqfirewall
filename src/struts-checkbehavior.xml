<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 业务级配置 BEGIN -->
	<package name="web-checkbehavior" extends="web-default"  namespace="/checkbehavior">
		<action name="index" class="SystemAction" method="IndexPage"><result name="index">/${jspFolder}/checkbehavior/index.jsp</result></action>
		<action name="det" class="SystemAction" method="IndexPage"><result name="index">/${jspFolder}/checkbehavior/det.jsp</result></action>
		<action name="new" class="SystemAction" method="IndexPage"><result name="index">/${jspFolder}/checkbehavior/new.jsp</result></action>
		<action name="mod" class="SystemAction" method="IndexPage"><result name="index">/${jspFolder}/checkbehavior/mod.jsp</result></action>
		<action name="det" class="SystemAction" method="IndexPage"><result name="index">/${jspFolder}/checkbehavior/det.jsp</result></action>
	</package>
	<package name="web-checkbehavior-json" extends="web-default-ajax" namespace="/checkbehavior">
		<action name="list" class="CheckbehaviorAction" method="List"></action>
		<action name="detail" class="CheckbehaviorAction" method="Det"></action>
		<action name="saveNew" class="CheckbehaviorAction" method="New"></action>
		<action name="saveMod" class="CheckbehaviorAction" method="Mod"></action>
		<action name="saveDel" class="CheckbehaviorAction" method="Del"></action>
	</package>
	<!-- 业务级配置 BEGIN -->
</struts>

